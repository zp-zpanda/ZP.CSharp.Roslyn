{
  "metadata": [
    {
      "src" : [
        {
          "files" : [
            "ZP.CSharp.Roslyn.dll",
            "ZP.CSharp.Roslyn.Docs.dll",
            "ZP.CSharp.Roslyn.Tests.dll"
          ],
          "src" : "./docs-bin"
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "docs/**",
          "old-docs/**"
        ]
      }
    ],
    "dest" : "docs",
    "globalMetadata" : {
      "_appTitle" : "ZP.CSharp.Roslyn",
      "_gitContribute" : {
        "repo" : "https://github.com/zp-zpanda/zp.csharp.roslyn",
        "branch" : "main"
      }
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template" : [
      "statictoc",
      "memberpage"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "xrefService" : [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ],
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "force" : true
  }
}
